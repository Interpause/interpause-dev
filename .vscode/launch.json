	"version": "0.2.0",
	"configurations": [
		{
			"name": "dev",
			"request": "launch",
			"runtimeArgs": ["dev"],
			"runtimeExecutable": "yarn",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},
		{
			"name": "build",
			"request": "launch",
			"runtimeArgs": ["build"],
			"runtimeExecutable": "yarn",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},
		{
			"name": "start",
			"request": "launch",
			"runtimeArgs": ["start"],
			"runtimeExecutable": "yarn",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		},
		{
			"name": "export",
			"request": "launch",
			"runtimeArgs": ["next","export"],
			"runtimeExecutable": "yarn",
			"skipFiles": [
				"<node_internals>/**"
			],
			"type": "pwa-node"
		}
		
	]
}